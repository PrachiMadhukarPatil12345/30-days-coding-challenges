#include <stdio.h> 
 
int main() { 
  int size; 
  int i, mid; 
  int array[50]; 

 printf ("PRINT MIDDLE ELEMENT FROM ARRAY IN C \n ");
  printf("Enter the size of the array: "); 
  scanf("%d", &size); 
 
  printf("Enter the elements of the array: "); 
  for (i = 0; i < size; i++) { 
    scanf("%d", &array[i]); 
  } 
 
  mid = size / 2; 
 
  printf("The middle element is %d\n", array[mid]); 
 
  return 0; 
}
