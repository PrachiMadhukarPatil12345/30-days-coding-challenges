#include<stdio.h>
#include<math.h>
int main()
{
   int x, y;
   printf("Enter two numbers: ");
   scanf("%d %d",&x, &y);

   int max = ((x+y)+abs(x-y))/2;
   int min = ((x+y)-abs(x-y))/2;

   printf("Maximum value = %d,max)                               
   printf ("\n Minimum value = %d",min);

   return 0;
}
